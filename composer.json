{
    "name": "ramesh-dada/flarum-increase-title",
    "description": "Increase Title Limit in Flarum to 180 characters",
    "keywords": [
      "flarum",
      "increase-title"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [{
      "name": "RameshDada",
      "homepage": "https://github.com/ramesh-dada/flarum-increase-title"
    }],
    "support": {
      "issues": "https://github.com/ramesh-dada/flarum-increase-title/issues",
      "source": "https://github.com/ramesh-dada/flarum-increase-title"
    },
    "require": {
      "flarum/core": "^1.0"
    },
    "autoload": {
      "psr-4": {
        "ramesh-dada\\flarum-increase-title\\": "src/"
      }
    },
    "extra": {
      "flarum-extension": {
        "title": "Extend Title",
        "icon": {
          "name": "fas fa-chart-line",
          "backgroundColor": "#17b2ff",
          "color": "#fff"
        }
      }
    }
  }
